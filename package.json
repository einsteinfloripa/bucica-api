{
  "name": "bucica-back",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "commit": "cz",
    "lint": "eslint src/**/*.ts",
    "prod:build": "babel src --extensions '.js,.ts' --out-dir build --no-copy-ignored",
    "prod:server": "node build/server.js",
    "prod:load-env": "dotenv -e .env.prod",
    "prod:db:push": "npx prisma migrate deploy",
    "prod:start": "npm run prod:load-env -- npm run prod:db:push && npm run prod:build && npm run prod:load-env -- npm run prod:server",
    "prod:docker": "docker-compose -f docker-compose.prod.yml --env-file .env.prod up -d",
    "dev": "tsnd --transpile-only --respawn --ignore-watch node_modules --exit-child --rs -r tsconfig-paths/register src/server.ts",
    "dev:test": "npm run test:load-env -- npm run test:db:push && npm run test:load-env -- vitest run",
    "dev:env": "npm run dev:load-env -- npm run dev",
    "dev:load-env": "dotenv -e .env.dev",
    "dev:docker": "docker-compose -f docker-compose.dev.yml --env-file .env.dev up -d",
    "migrate:dev": "npm run dev:load-env -- npx prisma db push --force-reset && npm run dev:load-env -- npx prisma db seed",
    "test:ci": "npx prisma migrate reset --force && vitest run",
    "test:load-env": "dotenv -e .env.test",
    "test:db:push": "npx prisma migrate reset --force --skip-seed"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "cors": "^2.8.5",
    "date-fns": "^2.29.2",
    "dayjs": "^1.11.5",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "prisma": "^4.3.1"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-typescript": "^7.18.6",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "commitizen": "^4.2.5",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.23.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-module-resolver": "^1.5.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "faker": "^6.6.6",
    "supertest": "^6.2.4",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.8.2",
    "vitest": "^0.22.1"
  }
}
